<script type="text/javascript">
    function check_date_under_year(sender,args)
    {
        fromdat=document.getElementById(sender.fromDate).value;
        todat=document.getElementById(sender.toDate).value;
        fromday=fromdat.substring(0,2);
        frommon=fromdat.substring(3,5);
        fromyr=fromdat.substring(6,10);
        tody=todat.substring(0,2);
        tomon=todat.substring(3,5);
        toyr=todat.substring(6,10);
        sender.innerHTML = "From Date cannot be greater than To Date";
        var from = fromdat.split("/");
        var date1 = new Date(from[2],from[1]-1,from[0]);
        from = todat.split("/");
        var date2 = new Date(from[2],from[1]-1,from[0]);
        var timeDiff = Math.abs(date2.getTime() - date1.getTime());
        var diffDays = Math.ceil(timeDiff / (1000 * 3600 * 24));         
        var nowdate = new Date();
        if(date2 > nowdate)
        {
            sender.innerHTML = "To Date cannot be greater than today";
             args.IsValid=false;
        }
        else if ( fromyr > toyr)
        {
            
            args.IsValid=false;
        } 	
        else if ((frommon > tomon) && (fromyr >= toyr))
        {
            args.IsValid=false;
        }
        else if ((fromday > tody) && (frommon >=tomon) && (fromyr >= toyr))
        {
            
            args.IsValid=false;
        }
        else{		
           
            
            if(diffDays>365)
            {
                sender.innerHTML = "Search period is restricted to one year only";
                args.IsValid=false;
            }
            else
            {
            args.IsValid=true;
            }
        }
        
    }
    function validate(key)
    {
        //getting key code of pressed key
        var keycode = (key.which) ? key.which : key.keyCode;
        if (!(keycode==8 || keycode==46)&&(keycode < 48 || keycode > 57) || keycode == 9 || keycode == 13)
        {
        return false;
        }
    }
</script>
