{
  "rules": {
    "no-undef": "off",
    "react/prop-types" : "off",
    "react/no-unescaped-entities": 0
  },
  "extends": [
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "requireConfigFile": false,
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "sourceType": "module",
    "allowImportExportEveryWhere": false,
    "ecmaFeatures": {
      "globalReturn": false,
      "jsx": true
    },
    "ecmaVersion": 2020
}
}


